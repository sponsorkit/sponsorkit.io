{
    "presets": [
      [
        "babel-preset-gatsby",
        {
          "reactRuntime": "automatic",
          "targets": {
            "browsers": ["last 1 versions", "not ie <= 11"]
          }
        }
      ]
    ],
    "plugins": [
      ["@babel/plugin-proposal-class-properties", { "loose": true }],
      ["@babel/plugin-transform-classes", {"loose": true}]
    ]
  }